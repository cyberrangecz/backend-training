{
  "template": "*",
  "index_patterns": ["kypo*"],
  "mappings": {
    "default": {
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "epoch_millis"
        },
        "start_time": {
          "type": "date",
          "format": "epoch_millis"
        },
        "end_time": {
          "type": "date",
          "format": "epoch_millis"
        }
      }
    }
  },
  "settings": {
    "index.refresh_interval": "5s"
  }
}