{
  "template": "*",
  "index_patterns": ["kypo.*"],
  "mappings": {
      "properties": {
        "timestamp": {
          "type": "long", "fields": {
            "ts": {
              "type": "date", "format": "epoch_millis"
            }
          }
        }
    }
  },
  "settings": {
    "index.refresh_interval": "5s"
  }
}
