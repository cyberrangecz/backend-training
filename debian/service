[Unit]
Description=KYPO Training service
After=syslog.target

[Service]
User=kypo
ExecStart=/usr/bin/java -Dpath.to.config.file=/etc/kypo2/java-projects/properties/training/training-project.properties -jar /opt/kypo2/kypo2-training/jars/kypo2-rest-training.jar
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=kypo2-training
Restart=no

[Install]
WantedBy=multi-user.target
